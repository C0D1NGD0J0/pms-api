{
  "name": "property-management-system",
  "version": "0.0.1",
  "description": "Property management system that helps landlord streamline rental payment, property maintenance etc...",
  "main": "server.js",
  "engines": {
    "node": ">=22.0.0 <23.0.0"
  },
  "scripts": {
    "build": "rm -rf ./dist && tsc -p ./tsconfig.build.json && tsc-alias && mkdir -p dist/app/templates/lease && cp -r app/templates/lease/* dist/app/templates/lease/",
    "prettier:check": "prettier --check './app/**/*.{ts,json,js}' './tests/**/*.{ts,json,js}'",
    "prettier:write": "prettier --write './app/**/*.{ts,json,js}' './tests/**/*.{ts,json,js}'",
    "lint:fix": "eslint './app/**/*.{ts,js}' './tests/**/*.{ts,js}' --fix",
    "lint": "eslint './app/**/*.{ts,js}' './tests/**/*.{ts,js}'",
    "prepare": "husky",
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:worker\" | ./node_modules/.bin/bunyan",
    "dev:api": "nodemon --exec 'node --inspect=9229 --expose-gc -r ts-node/register' server.ts",
    "dev:worker": "nodemon --exec 'node --inspect=9230 --expose-gc -r ts-node/register' worker_process.ts",
    "start:api": "node dist/server.js",
    "start:worker": "node dist/worker.js",
    "start": "node dist/server.js",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:unit": "cross-env NODE_ENV=test jest --testPathPattern=\"tests/unit/${npm_config_path:-}\"",
    "test:integration": "cross-env NODE_ENV=test jest --config test.config.integration.ts",
    "test:integration:watch": "cross-env NODE_ENV=test jest --config test.config.integration.ts --watch",
    "seed:db": "ts-node -r tsconfig-paths/register scripts/seeds/seed-database.ts",
    "seed:clean": "ts-node -r tsconfig-paths/register scripts/seeds/seed-database.ts --clean"
  },
  "keywords": [
    "property-management-system",
    "nodejs",
    "mongodb",
    "stripe"
  ],
  "author": "C0D1NGD0J0",
  "license": "ISC",
